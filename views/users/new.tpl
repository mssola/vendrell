
<div class="dialog">
    <div class="dialog-header">
        <h1>Crear usuari</h1>
    </div>
    <div class="dialog-body">
        <form action="/users" method="POST" autocomplete="off" accept-charset="utf-8">
            <input class="text" type="text" name="name" tabindex="1" autofocus placeholder="Nom" />
            <input class="text" type="password" name="password" tabindex="2"  autofocus placeholder="Contrasenya" />
            <input class="yellow-btn" type="submit" value="Crear" />
            <div class="clearout"></div>
        </form>
    </div>
</div>

